{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://https://api.vrchat.cloud/api/1/auth/user" }]
    }
  ]
}